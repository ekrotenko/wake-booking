{
  "name": "wake.booking.api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precommit": "npm run lint",
    "lint": "./node_modules/.bin/eslint ./*/**.js *.js",
    "lint:fix": "./node_modules/.bin/eslint ./*/**.js *.js --fix",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "seed:all": "sequelize db:seed:all",
    "seed:undo:all": "sequelize db:seed:undo:all",
    "debug": "nodemon --inspect=0.0.0.0:5858 app.js",
    "start": "node ./app.js",
    "start:local": "export NODE_ENV=local && npm run start",
    "seed": "node seed.js",
    "test": "./node_modules/.bin/jasmine ./**/*.spec.js"
  },
  "dependencies": {
    "@ssense/sscheduler": "^1.3.2",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.2",
    "cookie-parser": "~1.4.3",
    "crypto": "^1.0.1",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-promise": "^0.4.0",
    "faker": "^4.1.0",
    "jade": "~1.11.0",
    "jwt-simple": "^0.5.1",
    "moment": "^2.22.1",
    "morgan": "~1.8.1",
    "mysql": "^2.14.1",
    "mysql2": "^1.5.3",
    "nconf": "^0.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "sequelize": "^4.37.6",
    "sequelize-auto-migrations": "^1.0.2",
    "sequelize-restful-helper": "^1.0.2",
    "serve-favicon": "~2.4.2",
    "volleyball": "^1.4.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.11.0",
    "husky": "^0.14.3",
    "jasmine": "^3.1.0",
    "jasmine-data-provider": "^2.2.0",
    "nodemon": "^1.17.3",
    "random-string": "^0.2.0",
    "request-promise": "^4.2.2",
    "sequelize-cli": "^4.0.0"
  }
}
