{
  "name": "wake.booking.api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precommit": "npm run lint && npm run test",
    "lint": "./node_modules/.bin/eslint ./*/**.js *.js",
    "lint:fix": "./node_modules/.bin/eslint ./*/**.js *.js --fix",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all",
    "debug": "nodemon --inspect=0.0.0.0:5858 app.js",
    "start": "node ./app.js",
    "prestart:local": "export NODE_ENV=test && npm run migrate",
    "start:local": "export NODE_ENV=test && npm run start",
    "pretest": "export NODE_ENV=test && npm run migrate:undo && npm run migrate",
    "test": "export NODE_ENV=test && ./node_modules/.bin/jasmine ./**/*.spec.js"
  },
  "dependencies": {
    "@ssense/sscheduler": "^1.3.2",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "crypto": "^1.0.1",
    "debug": "~2.6.3",
    "express": "^4.16.4",
    "express-promise": "^0.4.0",
    "faker": "^4.1.0",
    "jade": "~1.11.0",
    "joi": "^14.3.0",
    "joi-date-extensions": "^1.2.0",
    "jwt-simple": "^0.5.5",
    "lodash.capitalize": "^4.2.1",
    "moment": "^2.22.2",
    "moment-range": "^4.0.1",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "nconf": "^0.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "sequelize": "^4.41.2",
    "sequelize-auto-migrations": "^1.0.2",
    "sequelize-restful-helper": "^1.0.2",
    "serve-favicon": "~2.4.2",
    "volleyball": "^1.5.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.14.3",
    "jasmine": "^3.3.0",
    "jasmine-data-provider": "^2.2.0",
    "nodemon": "^1.18.7",
    "random-string": "^0.2.0",
    "request-promise": "^4.2.2",
    "sequelize-cli": "^4.1.1",
    "supertest": "^3.3.0"
  }
}
