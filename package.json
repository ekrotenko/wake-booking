{
  "name": "wake.booking.api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "migrate:local": "sequelize db:migrate --env local",
    "seed:all:local": "sequelize db:seed:all --env local",
    "seed:undo:all:local": "sequelize db:seed:undo:all --env local",

    "debug": "nodemon --inspect=0.0.0.0:5858 app.js",
    "start": "nodemon app.js",
    "start:local": "export NODE_ENV=local && npm run start",
    "seed": "node seed.js"
  },
  "dependencies": {
    "@ssense/sscheduler": "^1.3.2",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "cookie-parser": "~1.4.3",
    "crypto": "^1.0.1",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "faker": "^4.1.0",
    "jade": "~1.11.0",
    "jwt-simple": "^0.5.1",
    "moment": "^2.21.0",
    "morgan": "~1.8.1",
    "mysql": "^2.14.1",
    "mysql2": "^1.5.3",
    "nconf": "^0.9.1",
    "nodemon": "^1.17.2",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "sequelize": "^4.37.5",
    "sequelize-auto-migrations": "^1.0.2",
    "sequelize-cli": "^4.0.0",
    "sequelize-restful-helper": "^1.0.2",
    "serve-favicon": "~2.4.2",
    "volleyball": "^1.4.1"
  }
}
